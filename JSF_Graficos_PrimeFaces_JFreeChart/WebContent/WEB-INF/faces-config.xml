<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>vendaBean</managed-bean-name>
		<managed-bean-class>com.livro.capitulo12.primefaces.pizza.VendaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>vendaColunasBean</managed-bean-name>
		<managed-bean-class>com.livro.capitulo12.primefaces.colunas.VendaColunasBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>votoBean</managed-bean-name>
		<managed-bean-class>com.livro.capitulo12.primefaces.enquete.VotoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>cidadeBean</managed-bean-name>
		<managed-bean-class>com.livro.capitulo12.jfreechart.pizza.CidadeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>cidadeBarrasBean</managed-bean-name>
		<managed-bean-class>com.livro.capitulo12.jfreechart.colunas.CidadeColunasBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

</faces-config>
